<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Other Operations">

    <listeners>
        <listener class-name="listeners.CalcTestListener"></listener>
    </listeners>

    <test name="Pow Tests">
        <classes>
            <class name="com.epam.cdp.calc_unit_tests.tests.PowCalcTestDouble"></class>
        </classes>
    </test>

    <test name="Sqrt Tests">
        <classes>
            <class name="com.epam.cdp.calc_unit_tests.tests.SqrtCalcTestDouble"></class>
        </classes>
    </test>

    <test name="Trigonometry Tests">
        <classes>
            <class name="com.epam.cdp.calc_unit_tests.tests.SinCalcTestDouble"></class>
            <class name="com.epam.cdp.calc_unit_tests.tests.CosCalcTestDouble"></class>
            <class name="com.epam.cdp.calc_unit_tests.tests.TgCalcTestDouble"></class>
            <class name="com.epam.cdp.calc_unit_tests.tests.CtgCalcTestDouble"></class>
        </classes>
    </test>

    <test name="Boolean tests">
        <classes>
            <class name="com.epam.cdp.calc_unit_tests.tests.IsPositiveCalcTestLong"></class>
            <class name="com.epam.cdp.calc_unit_tests.tests.IsNegativeCalcTestLong"></class>
        </classes>
    </test>
    
</suite>