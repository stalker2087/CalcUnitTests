<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Other Operations">

    <listeners>
        <listener class-name="listeners.CalcTestListener"></listener>
    </listeners>

    <test name="Pow Tests">
        <classes>
            <class name="com.epam.cdp.CalcUnitTests.Tests.PowCalcTestDouble"></class>
        </classes>
    </test>

    <test name="Sqrt Tests">
        <classes>
            <class name="com.epam.cdp.CalcUnitTests.Tests.SqrtCalcTestDouble"></class>
        </classes>
    </test>

    <test name="Trigonometry Tests">
        <classes>
            <class name="com.epam.cdp.CalcUnitTests.Tests.SinCalcTestDouble"></class>
            <class name="com.epam.cdp.CalcUnitTests.Tests.CosCalcTestDouble"></class>
            <class name="com.epam.cdp.CalcUnitTests.Tests.TgCalcTestDouble"></class>
            <class name="com.epam.cdp.CalcUnitTests.Tests.CtgCalcTestDouble"></class>
        </classes>
    </test>

    <test name="Boolean tests">
        <classes>
            <class name="com.epam.cdp.CalcUnitTests.Tests.IsPositiveCalcTestLong"></class>
            <class name="com.epam.cdp.CalcUnitTests.Tests.IsNegativeCalcTestLong"></class>
        </classes>
    </test>
    
</suite>